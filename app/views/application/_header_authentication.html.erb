<% if !account_signed_in? %>
	<li class="<%= cp('new_account_registration_path') %>"><%= link_to 'Sign Up', new_account_registration_path %></li>
	<li class="<%= cp('new_account_session_path') %>"><%= link_to 'Sign In', new_account_session_path %></li>
<% else %>
    <li><%= link_to 'Profile', current_account %></li>
	<li><%= link_to 'Sign Out', destroy_account_session_path, method: :delete %></li>
<% end %>