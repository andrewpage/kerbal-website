<%= form_for @mod do |f| %>
	<%= f.label :name %> <%= f.text_field :name %> <br />
	<%= f.label 'Short Description' %> <%= f.text_area :description_short %> <br />
	<%= f.label 'Description' %> <%= f.text_area :description %> <br />
	<%= f.label :version %> <%= f.text_field :version %> <br />
	<%= f.label :tags %> <%= f.text_field :tags %> <br />
    <%= f.label :image %><%= f.file_field :image %> <br />
    <%= f.label 'Youtube Link' %> <%= f.text_field :youtube_link %> <br />
	<%= f.submit :create %>
<% end %>